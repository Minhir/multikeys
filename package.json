{
  "name": "multikeys",
  "version": "1.0.0",
  "description": "Multikeys data structures collection",
  "author": "Vladimir Ivakin",
  "license": "MIT",
  "homepage": "https://github.com/Minhir/multikeys",
  "keywords": [
    "multikey",
    "multiple key",
    "map",
    "set"
  ],
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "tsc",
    "build:doc": "cat description.md > README.md && jsdoc2md --files ./src/*.ts --configure ./jsdoc2md.json >> README.md",
    "build:doc:ci": "cat description.md > README_BASE.md && jsdoc2md --files ./src/*.ts --configure ./jsdoc2md.json >> README_BASE.md && cmp --silent README.md README_BASE.md || (echo 'Update README.md. Run \"npm run build:doc\" and add changes into PR.' && exit 1)",
    "lint": "eslint ./src",
    "test": "jest",
    "test:coverage": "npm run test -- --collect-coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "jest": "^26.4.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.6"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Minhir/multikeys.git"
  }
}
